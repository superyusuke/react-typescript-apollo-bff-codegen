generator client {
  provider      = "prisma-client-js"
  output        = "../src/generated/prismaClient"
  binaryTargets = ["native", "darwin", "debian-openssl-1.1.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model account {
  user_id  Int     @default(autoincrement()) @id
  username String?
}

